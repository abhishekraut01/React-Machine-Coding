{
  "manifest_version": 3,
  "name": "Aifiesta API Requester",
  "version": "1.0",
  "description": "Send custom chat/completions request to Aifiesta",
  "permissions": [
    "storage",
    "clipboardRead",
    "scripting"
  ],
  "host_permissions": [
    "https://api-v2.aifiesta.ai/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
